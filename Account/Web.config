<?xml version="1.0"?>
<configuration>
<defaultDocument> 
          <files>                           
             <clear/>                           
             <add value="welcome.aspx"/>                      
          </files>
</defaultDocument>
  <location path="Register.aspx">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>

  <system.web>
    <authorization>
      <deny users="?"/>
    </authorization>
  </system.web>

</configuration>
