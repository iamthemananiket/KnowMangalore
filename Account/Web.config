<?xml version="1.0"?>
<configuration>
<system.webServer>
  <defaultDocument>
    <files>
      
      <add value="welcome.aspx" />
    </files>
  </defaultDocument>
</system.webServer>
  <location path="Register.aspx">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>

  <system.web>
    <authorization>
      <deny users="?"/>
    </authorization>
  </system.web>

</configuration>
